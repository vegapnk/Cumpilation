<?xml version="1.0" encoding="utf-8"?>

<Defs>
    <ThingCategoryDef>
        <defName>Cumpilation_BodilyFluid</defName>
        <label>bodily fluid</label>
        <description>Items that are considered bodily fluids.</description>
    </ThingCategoryDef>

    <ThingDef ParentName="StoneBlocksBase">
        <defName>Cumpilation_Samenstein</defName>
        <label>samenstein</label>
        <description>A light and resilient but not durable material, samenstein has its use by being
            a very obtainable material. Made of processed semen, it can eventually be made by anyone
            capable of ejaculating.</description>
        <graphicData>
            <texPath>Things/Resource/Samenstein</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(252, 251, 237)</color>
        </graphicData>
        <statBases>
            <Mass>0.6</Mass>
            <SharpDamageMultiplier>0.65</SharpDamageMultiplier>
        </statBases>
        <burnableByRecipe>true</burnableByRecipe>
        <stuffProps>
            <color>(252, 251, 237)</color>
            <soundMeleeHitSharp>MeleeHit_Stone</soundMeleeHitSharp>
            <soundMeleeHitBlunt>MeleeHit_Stone</soundMeleeHitBlunt>
            <stuffAdjective>samensteiny</stuffAdjective>
            <statFactors>
                <MaxHitPoints>1.6</MaxHitPoints>
                <Beauty>-1</Beauty>
                <Flammability>1.5</Flammability>
                <WorkToMake>0.8</WorkToMake>
                <WorkToBuild>0.8</WorkToBuild>
                <DoorOpenSpeed>1.2</DoorOpenSpeed>
            </statFactors>
            <canSuggestUseDefaultStuff>true</canSuggestUseDefaultStuff>
        </stuffProps>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    </ThingDef>


    <ThingDef ParentName="ResourceBase">
        <defName>Cumpilation_Spermatuch</defName>
        <label>spermatuch</label>
        <description>Fabric microscopically spun from microfibers made from semen. It is very tough,
            good at insulating, but highly flammable.</description>
        <graphicData>
            <texPath>Things/Resource/Spermatuch</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
            <MaxHitPoints>100</MaxHitPoints>
            <MarketValue>5.1</MarketValue>
            <Mass>0.032</Mass>
            <Flammability>1.8</Flammability>
            <DeteriorationRate>3</DeteriorationRate>
            <StuffPower_Armor_Sharp>1.90</StuffPower_Armor_Sharp>
            <StuffPower_Armor_Blunt>0.36</StuffPower_Armor_Blunt>
            <StuffPower_Armor_Heat>3.00</StuffPower_Armor_Heat>
            <StuffPower_Insulation_Cold>26</StuffPower_Insulation_Cold>
            <StuffPower_Insulation_Heat>20</StuffPower_Insulation_Heat>
        </statBases>
        <burnableByRecipe>true</burnableByRecipe>
        <healthAffectsPrice>false</healthAffectsPrice>
        <minRewardCount>30</minRewardCount>
        <stuffProps>
            <categories>
                <li>Fabric</li>
            </categories>
            <commonality>0.20</commonality>
            <color>(252, 250, 240)</color>
            <statFactors>
                <Flammability>1.8</Flammability>
                <MaxHitPoints>1.3</MaxHitPoints>
                <Beauty>3.2</Beauty>
            </statFactors>
        </stuffProps>
        <thingCategories>
            <li>Textiles</li>
        </thingCategories>
        <comps>
            <li Class="CompProperties_ColorableAnimated">
                <colors>
                    <li>(255, 255, 255)</li>
                    <li>(252, 250, 240)</li>
                    <li>(200, 200, 200)</li>
                </colors>
            </li>
        </comps>
        <allowedArchonexusCount>80</allowedArchonexusCount>
    </ThingDef>
</Defs>